{
  "env": {
      "browser": true,
      "commonjs": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "max-len": [
      "warn",
      {
        "code": 110,
        "tabWidth": 2,
        "comments": 110,
        "ignoreComments": false,
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ]
  }
}
